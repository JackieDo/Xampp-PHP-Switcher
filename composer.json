{
    "name": "jackiedo/xampp-php-switcher",
    "description": "Allow to use and switch between different versions of PHP for Xampp on Windows OS.",
    "keywords": [
        "xampp",
        "xampp-windows",
        "php",
        "php-switcher",
        "php-switch",
        "console"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "files": [
            "src/pre_defined.php",
            "src/helpers.php"
        ],
        "psr-4": {
            "PHPSwitcher\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "xphp.bat install"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}